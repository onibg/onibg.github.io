var size=1098893321,n=20,j
function read(i){
 let xhr=new XMLHttpRequest()
 xhr.responseType='arraybuffer'
 xhr.open('GET',url,true)
 let max=Math.floor(size*Math.random())//max最大到size-1
 xhr.setRequestHeader('Range',`bytes=${max-99999}-${max}`)
 xhr.send()
 xhr.onload=()=>{j++
  console.clear()
  console.log(`總數:${j},第${i}`,new Uint8Array(xhr.response))
  if(j==n){go()}
 }
}


go()
function go(){j=0
 for(let i=1;i<=n;i++){
  read(i)
 }
}
